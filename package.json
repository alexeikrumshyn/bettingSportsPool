  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "cron": "^1.8.2",
    "datejs": "^1.0.0-rc3",
    "express": "^4.16.2",
    "firebase": "^8.2.1",
    "node-fetch": "^3.1.1",
    "request": "^2.88.2"
  }
}
